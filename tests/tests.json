{
  "setups": {
    "default": {
      "posts": ["laserpost-lightburn", "laserpost-svg"],
      "options": {
        "quiet": "--quiet",
        "nobackup": "--nobackup",
        "noeditor": "--noeditor",
        "nointeraction": "--nointeraction",
        "timeout": ["--timeout", "1"]
      },
      "cnc": "test",
      "validate": [
        { "type": "snapshot" },
        { "type": "match", "value": "error", "max": 0 },
        { "type": "match", "value": "warning", "max": 0 },
        { "type": "match", "value": "copyright", "min": 1 }
      ]
    },
    "defaultBasic": {
      "setup": "default",
      "properties": {
        "work0100TraceStock": "0"
      }
    },
    "stockTrace": {
      "setup": "defaultBasic",
      "properties": {
        "work0100TraceStock": "1"
      }
    },
    "workOffset": {
      "setup": "defaultBasic",
      "options": {
        "nobackup": null
      },
      "properties": {
        "work0200OffsetX": "100",
        "work0300OffsetY": "200"
      }
    }
  },
  "tests": [
    {
      "name": "Default basic test",
      "setup": "default"
    },
    {
      "name": "Trace stock LBRN only",
      "setup": "workOffset",
      "properties": {
        "work0100TraceStock": "1"
      },
      "posts": ["laserpost-lightburn"]
    }
  ]
}
